pipeline {
    agent { any { image 'maven:3.3.3' } }
    
    stages {

        stage('log version info') {
            steps {
                // Compilar la aplicaci√≥n Spring Boot usando Maven Wrapper
                sh 'mvn --version'
                sh './mvnw clean package'
            }
        }
        stage('Test') {
            steps {
                // Ejecutar pruebas
                sh './mvnw test'
            }
        }
}
